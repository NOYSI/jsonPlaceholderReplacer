    "replacer",
    "substitution",
    "placeholder",
    "cli",
    "library",
    "typescript",
    "javascript",
    "javascript-library"
  ],
  "author": "Virgs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lopidio/jsonPlaceholderReplacer/issues"
  },
  "homepage": "https://github.com/lopidio/jsonPlaceholderReplacer#readme",
  "devDependencies": {
    "@types/jest": "^22.2.0",
    "@types/node": "^9.4.6",
    "jest": "^22.4.2",
    "ts-jest": "^22.4.1",
    "ts-node": "^4.1.0",
    "tsd": "^0.6.5",
    "typescript": "^2.7.2"
  },
  "bin": {
    "json-placeholder-replacer": "dist/index.js"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
