{
  "name": "json-placeholder-replacer",
  "version": "1.0.11",
  "description": "Javascript/Typescript library/cli to replace placeholders in json",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "tsc"
  },
